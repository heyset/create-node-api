  "devDependencies": {
    "@types/node": "^16.11.5",
    "ts-loader": "^9.2.6",
    "type-fest": "^2.5.1",
    "typescript": "^4.4.4",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
